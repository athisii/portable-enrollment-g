                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <ComboBox fx:id="comboUnitId" layoutX="518.0" layoutY="419.0" prefHeight="26.0" prefWidth="201.0"/>
                <AnchorPane layoutY="711.0" prefHeight="30.0" prefWidth="1024.0"
                            style="-fx-background-color: #1990ea;"/>
                <AnchorPane prefHeight="100.0" prefWidth="1024.0" style="-fx-background-color: #1990ea;">

                    <ImageView fitHeight="91.0" fitWidth="363.0" layoutX="17.0" layoutY="3.0" pickOnBounds="true"
