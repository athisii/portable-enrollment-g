                        textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Text fill="#04127b" layoutX="326.0" layoutY="437.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Enrollment Station Unit ID :"/>
                <Text fill="#04127b" layoutX="326.0" layoutY="340.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Miscos API Server URL :"/>
                <Text fill="#04127b" layoutX="326.0" layoutY="390.0" strokeType="OUTSIDE" strokeWidth="0.0"
