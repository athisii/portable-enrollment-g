<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cdac.enrollmentstation.PrimaryController">

    <BorderPane prefHeight="778.0" prefWidth="1024.0">
        <top>
            <AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #fff;"
                        BorderPane.alignment="CENTER">

                <AnchorPane prefHeight="100.0" prefWidth="1024.0" style="-fx-background-color: #1990ea;">

                    <ImageView fitHeight="91.0" fitWidth="363.0" layoutX="17.0" layoutY="3.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/navylogo.png"/>

                    </ImageView>
                    <Label alignment="CENTER" layoutX="876.0" layoutY="42.0" prefHeight="31.0" prefWidth="108.0"
                           style="-fx-background-color: #fff; -fx-background-radius: 0 20 20 0; -fx-border-width: 1; -fx-border-color: #00aaff; -fx-border-radius: 0 20 20 0;"
                           text="INS INDIA" textAlignment="CENTER" textFill="#030303" visible="false"/>

                </AnchorPane>
                <AnchorPane layoutY="728.0" prefHeight="30.0" prefWidth="1024.0" style="-fx-background-color: #1990ea;">

                    <Label fx:id="version" layoutX="506.0" layoutY="8.0"/>
                </AnchorPane>
                <AnchorPane layoutX="79.0" layoutY="234.0" prefHeight="300.0" prefWidth="300.0">

                    <ImageView fitHeight="168.0" fitWidth="188.0" layoutX="65.0" layoutY="51.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/biofirst.png"/>

                    </ImageView>
                    <Button layoutX="36.0" layoutY="249.0" mnemonicParsing="false" onAction="#showEnrollmentHome"
                            prefHeight="37.0" prefWidth="217.0"
                            style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="BIOMETRIC ENROLLMENT" textFill="WHITE"/>

                </AnchorPane>
                <AnchorPane layoutX="379.0" layoutY="234.0" prefHeight="300.0" prefWidth="300.0">

                    <ImageView fitHeight="145.0" fitWidth="263.0" layoutX="18.0" layoutY="80.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/importexport.jpeg"/>

                    </ImageView>
                    <Button fx:id="importexport" layoutX="40.0" layoutY="249.0" mnemonicParsing="false"
                            onAction="#showImportExport" prefHeight="37.0" prefWidth="216.0"
                            style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="IMPORT/EXPORT" textFill="WHITE"/>

                </AnchorPane>
                <AnchorPane layoutX="679.0" layoutY="234.0" prefHeight="300.0" prefWidth="300.0">

                    <ImageView fitHeight="162.0" fitWidth="161.0" layoutX="66.0" layoutY="58.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/settingfirst.png"/>

                    </ImageView>
                    <Button layoutX="47.0" layoutY="249.0" mnemonicParsing="false" onAction="#showPasswdScreen"
                            prefHeight="37.0" prefWidth="216.0"
                            style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="APPLICATION SETTINGS" textFill="WHITE"/>

                </AnchorPane>
                <ImageView fitHeight="47.0" fitWidth="48.0" layoutX="21.0" layoutY="112.0" pickOnBounds="true"
                           preserveRatio="true">

                    <Image url="@img/home.png"/>

                </ImageView>
                <Label layoutX="66.0" layoutY="131.0" prefHeight="27.0" prefWidth="77.0" text="HOME">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <Label fx:id="logout" layoutX="946.0" layoutY="131.0" onMouseClicked="#OnLogout" prefHeight="27.0"
                       prefWidth="77.0" style="-fx-cursor: hand;" text="LOGOUT" underline="true">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <ImageView fitHeight="36.0" fitWidth="35.0" layoutX="901.0" layoutY="123.0" pickOnBounds="true"
                           preserveRatio="true">

                    <Image url="@img/logout.png"/>

                </ImageView>
                <Label fx:id="statusmsg" alignment="CENTER" layoutX="230.0" layoutY="570.0" prefHeight="27.0"
                       prefWidth="582.0">
                    <font>
                        <Font name="System Bold" size="17.0"/>
                    </font>
                </Label>

            </AnchorPane>
        </top>
    </BorderPane>

</VBox>
