<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cdac.enrollmentstation.VerifyAdminPasswd">

    <BorderPane prefHeight="778.0" prefWidth="1024.0">
        <center>
            <AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #fff;"
                        BorderPane.alignment="CENTER">

                <Button fx:id="previous" layoutX="376.0" layoutY="487.0" maxHeight="35.0" maxWidth="150.0"
                        minHeight="35.0" minWidth="150.0" mnemonicParsing="false" onAction="#showHome" prefHeight="35.0"
                        prefWidth="150.0" style="-fx-background-color: #f7a304; -fx-background-radius: 20 20 20 20;"
                        text="BACK" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Button fx:id="showServerConfig" layoutX="544.0" layoutY="487.0" maxHeight="35.0" maxWidth="150.0"
                        minHeight="35.0" minWidth="150.0" mnemonicParsing="false" onAction="#serverConfig"
                        prefHeight="35.0" prefWidth="150.0"
                        style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;" text="LOGIN"
                        textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <AnchorPane layoutY="711.0" prefHeight="30.0" prefWidth="1061.0"
                            style="-fx-background-color: #1990ea;"/>
                <AnchorPane prefHeight="100.0" prefWidth="1061.0" style="-fx-background-color: #1990ea;">

                    <ImageView fitHeight="91.0" fitWidth="363.0" layoutX="17.0" layoutY="3.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/navylogo.png"/>

                    </ImageView>
                    <Label alignment="CENTER" layoutX="876.0" layoutY="42.0" prefHeight="31.0" prefWidth="108.0"
                           style="-fx-background-color: #fff; -fx-background-radius: 0 20 20 0; -fx-border-width: 1; -fx-border-color: #00aaff; -fx-border-radius: 0 20 20 0;"
                           text="INS INDIA" textAlignment="CENTER" visible="false"/>

                </AnchorPane>
                <AnchorPane layoutY="100.0" prefHeight="60.0" prefWidth="1061.0" style="-fx-background-color: #04127b;">

                    <Label layoutX="448.0" layoutY="19.0" text="Enter Username &amp; Password" textFill="WHITE">
                        <font>
                            <Font size="16.0"/>
                        </font>
                    </Label>

                </AnchorPane>
                <AnchorPane layoutX="301.0" layoutY="251.0" prefHeight="144.0" prefWidth="487.0"
                            style="-fx-border-color: #04127b;">

                    <PasswordField fx:id="adminpwd" layoutX="154.0" layoutY="75.0" prefHeight="26.0" prefWidth="194.0"
                                   promptText="Password"/>
                    <TextField fx:id="user" layoutX="154.0" layoutY="22.0" prefHeight="26.0" prefWidth="190.0"
                               promptText="Username"/>
                    <ImageView fitHeight="43.0" fitWidth="41.0" layoutX="73.0" layoutY="15.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/username.png"/>

                    </ImageView>
                    <ImageView fitHeight="44.0" fitWidth="59.0" layoutX="72.0" layoutY="66.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@img/password.png"/>

                    </ImageView>

                </AnchorPane>
                <Label fx:id="statusMsg" alignment="CENTER" layoutX="274.0" layoutY="434.0" prefHeight="33.0"
                       prefWidth="553.0">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>

            </AnchorPane>
        </center>
    </BorderPane>

</VBox>
